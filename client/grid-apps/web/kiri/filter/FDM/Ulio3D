    "post":[
        "M107                ; turn off filament cooling fan",
        "M104 S0 T{tool}     ; turn off right extruder",
        "G1 Z{z_max} F180    ; drop bed",
        "G28 X0 Y0           ; home XY axes",
        "M84                 ; disable stepper motors"
